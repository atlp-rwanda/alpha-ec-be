{
  "name": "alpha-ec-be",
  "version": "1.0.0",
  "description": "alpha team project",
  "main": "index.js",
  "scripts": {
    "test": "npm run demigrate && npm run migrate && npm run test:coverage",
    "test:coverage": "cross-env NODE_ENV=test nyc --reporter=text --reporter=lcov mocha --require ts-node/register test/server-run.ts --coverage",
    "dev": "cross-env NODE_ENV=development nodemon",
    "build:test": "tsc --project test",
    "start": "cross-env NODE_ENV=production node ./dist/server.js",
    "migrate": "sequelize db:migrate ",
    "demigrate": "sequelize db:migrate:undo:all",
    "seed": "cross-env NODE_ENV=development sequelize db:seed:all",
    "coverage": "nyc --reporter=text npm run test --coverage",
    "lint": "eslint .",
    "prettier": "prettier --check .",
    "lint:staged": "lint-staged",
    "lint:fix": "eslint . --fix",
    "prettier:fix": "prettier --write .",
    "prettier:check": "prettier --check .",
    "prepare": "husky install"
  },
  "nodemonConfig": {
    "watch": [
      "src"
    ],
    "ext": "ts",
    "ignore": [
      "src/**/*.spec.ts"
    ],
    "exec": "cross-env NODE_ENV=development tsc && node ./dist/server.js"
  },
  "author": "alpha atlp team",
  "license": "ISC",
  "devDependencies": {
    "@types/chai": "^4.3.14",
    "@types/cors": "^2.8.17",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.21",
    "@types/mocha": "^10.0.6",
    "@types/node": "^20.11.30",
    "@types/pg": "^8.11.4",
    "@types/sinon": "^17.0.3",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^7.4.0",
    "@typescript-eslint/parser": "^7.4.0",
    "chai": "^4.4.1",
    "cross-env": "^7.0.3",
    "eslint": "^8.57.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsdoc": "^48.2.1",
    "eslint-plugin-mocha": "^10.4.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "husky": "^9.0.11",
    "istanbul": "^0.4.5",
    "lint-staged": "^15.2.2",
    "mocha": "^10.4.0",
    "nodemon": "^3.1.0",
    "nyc": "^15.1.0",
    "prettier": "^3.2.5",
    "sequelize-cli": "^6.6.2",
    "sinon": "^17.0.1",
    "supertest": "^6.3.4",
    "swagger-ui-express": "^5.0.0",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.3"
  },
  "dependencies": {
    "@babel/cli": "^7.24.1",
    "@babel/core": "^7.24.3",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-object-rest-spread": "^7.20.7",
    "@babel/plugin-transform-runtime": "^7.24.3",
    "@babel/preset-env": "^7.24.3",
    "@babel/register": "^7.23.7",
    "@types/bcrypt": "^5.0.2",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/sequelize": "^4.28.20",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.6",
    "axios": "^1.6.8",
    "chai-http": "^4.4.0",
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "joi": "^17.12.2",
    "jsonwebtoken": "^9.0.2",
    "nodemon": "^3.1.0",
    "express": "^4.19.2",
    "pg": "^8.11.3",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.37.1",
    "swagger": "^0.0.1",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0",
    "uuid": "^9.0.1",
    "winston": "^3.13.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run test && lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts}": [
      "eslint --fix",
      "prettier --write",
      "git add"
    ],
    "uuid": "^9.0.1",
    "winston": "^3.13.0"
  },
  "nyc": {
    "check-coverage": true,
    "per-file-ignores": [
      "src/config/*",
      "src/database/*"
    ],
    "include": [
      "src"
    ],
    "exclude": [
      "src/database/*",
      "src/config/*"
    ]
  }
}
